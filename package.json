{
  "name": "ag-grid-addons",
  "version": "1.1.0",
  "description": "ag-grid plugin for k2",
  "scripts": {
    "compile": "better-npm-run compile",
    "start": "better-npm-run start",
    "dev": "node example/server.js",
    "lint": "eslint src tests example *.js",
    "spec": "NODE_ENV=production babel-tape-runner tests/**/*.test.js | faucet",
    "test": "npm run lint && npm run spec",
    "build": "NODE_ENV=production babel src -d lib"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node example/server.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/k2data/ag-grid-addons.git"
  },
  "keywords": [
    "react"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/k2data/ag-grid-addons/issues"
  },
  "homepage": "https://github.com/k2data/ag-grid-addons",
  "dependencies": {
    "ag-grid": "^8.2.0",
    "ag-grid-react": "^8.2.0",
    "font-awesome": "^4.7.0",
    "moment": "^2.15.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "babel-tape-runner": "^2.0.1",
    "css-loader": "^0.27.3",
    "enzyme": "^2.4.1",
    "eslint": "^3.6.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-react": "^2.2.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-standard": "^1.3.1",
    "faucet": "0.0.1",
    "file-loader": "^0.10.1",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.1",
    "sinon": "^1.17.6",
    "style-loader": "^0.16.1",
    "tape": "^4.6.0",
    "url-loader": "^0.5.8",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  }
}
